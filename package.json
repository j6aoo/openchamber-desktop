{
  "name": "openchamber-desktop",
  "version": "1.0.0",
  "description": "Desktop launcher for OpenChamber - Cross-platform OpenCode GUI",
  "main": "bin/neutralino-linux_x64",
  "bin": {
    "openchamber-desktop": "./bin/cli.js",
    "ocd": "./bin/cli.js"
  },
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "start": "node bin/cli.js",
    "dev": "neu run",
    "build": "neu build",
    "build:release": "neu build --release",
    "build:appimage": "node scripts/build/build-appimage.js",
    "install:system": "node scripts/install/install.js"
  },
  "keywords": [
    "openchamber",
    "opencode",
    "ai",
    "coding-assistant",
    "desktop",
    "launcher",
    "neutralino"
  ],
  "author": "Aency Organization",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aencyorganization/openchamber-desktop.git"
  },
  "bugs": {
    "url": "https://github.com/aencyorganization/openchamber-desktop/issues"
  },
  "homepage": "https://github.com/aencyorganization/openchamber-desktop#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "files": [
    "bin/",
    "resources/",
    "neutralino.config.json",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@neutralinojs/neu": "^11.7.0",
    "neutralino-appimage-bundler": "^1.3.4"
  }
}
