{
  "applicationId": "com.openchamber.launcher",
  "version": "1.1.0",
  "defaultMode": "window",
  "url": "/resources/index.html",
  "port": 0,
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "exportAuthInfo": false,
  "nativeAllowList": [
    "app.*",
    "os.*",
    "window.*",
    "filesystem.*",
    "events.*"
  ],
  "modes": {
    "window": {
      "title": "OpenChamber Desktop (Unofficial)",
      "width": 900,
      "height": 700,
      "minWidth": 600,
      "minHeight": 400,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/icon.png",
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true
    }
  },
  "cli": {
    "binaryName": "openchamber-launcher",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "5.3.0",
    "clientVersion": "5.3.0"
  }
}
